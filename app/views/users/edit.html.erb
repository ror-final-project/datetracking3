<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

<%=form_for @user, :url => users_path, :html => { :multipart => true } do |f| %>
<%=f.file_field :avatar %>
<br>
<br>
<%=f.label :fname %>
<%=f.text_field :fname %>
<%=f.label :lname %>
<%=f.text_field :lname %>
<br>
<%=f.label :email %>
<%=f.email_field :email %>
<br>
<%=f.label :password %>
<%=f.password_field :password %>
<br>
<%=f.label :password_confirmation %>
<%=f.password_field :password %>
<br>
<%=f.submit "Save" %>
<% end %>